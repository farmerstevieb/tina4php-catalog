<form id="productAttributeTypeForm" enctype="multipart/form-data" xmlns="http://www.w3.org/1999/html">
    <div class="row">
                    <div class="form-group col-md-6">
                <label for="id">Id</label>
                <input class="form-control" type="text" id="id"
                       name="id" placeholder="Id"
                       value="{{ data.id }}">
            </div>
                    <div class="form-group col-md-6">
                <label for="product_id">Product_id</label>
                <input class="form-control" type="text" id="product_id"
                       name="product_id" placeholder="Product_id"
                       value="{{ data.product_id }}">
            </div>
            </div>
    {{"productAttributeTypeForm" | formToken | raw }}
</form>
