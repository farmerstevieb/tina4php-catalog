<form id="productVariantsForm" enctype="multipart/form-data" xmlns="http://www.w3.org/1999/html">
    <div class="row">
                    <div class="form-group col-md-6">
                <label for="productId">Product Id</label>
                <input class="form-control" type="text" id="productId"
                       name="productId" placeholder="Product Id"
                       value="{{ data.productId }}">
            </div>
                    <div class="form-group col-md-6">
                <label for="skuId">Sku Id</label>
                <input class="form-control" type="text" id="skuId"
                       name="skuId" placeholder="Sku Id"
                       value="{{ data.skuId }}">
            </div>
                    <div class="form-group col-md-6">
                <label for="attributeId">Attribute Id</label>
                <input class="form-control" type="text" id="attributeId"
                       name="attributeId" placeholder="Attribute Id"
                       value="{{ data.attributeId }}">
            </div>
                    <div class="form-group col-md-6">
                <label for="attributeValueId">Attribute Value Id</label>
                <input class="form-control" type="text" id="attributeValueId"
                       name="attributeValueId" placeholder="Attribute Value Id"
                       value="{{ data.attributeValueId }}">
            </div>
            </div>
    {{"productVariantsForm" | formToken | raw }}
</form>
