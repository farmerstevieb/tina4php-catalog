<form id="productSkusForm" enctype="multipart/form-data" xmlns="http://www.w3.org/1999/html">
    <div class="row">
                    <div class="form-group col-md-6">
                <label for="id">Id</label>
                <input class="form-control" type="text" id="id"
                       name="id" placeholder="Id"
                       value="{{ data.id }}">
            </div>
                    <div class="form-group col-md-6">
                <label for="productId">Product Id</label>
                <input class="form-control" type="text" id="productId"
                       name="productId" placeholder="Product Id"
                       value="{{ data.productId }}">
            </div>
                    <div class="form-group col-md-6">
                <label for="skuPrice">Sku Price</label>
                <input class="form-control" type="text" id="skuPrice"
                       name="skuPrice" placeholder="Sku Price"
                       value="{{ data.skuPrice }}">
            </div>
                    <div class="form-group col-md-6">
                <label for="skuName">Sku Name</label>
                <input class="form-control" type="text" id="skuName"
                       name="skuName" placeholder="Sku Name"
                       value="{{ data.skuName }}">
            </div>
                    <div class="form-group col-md-6">
                <label for="skuBarcode">Sku Barcode</label>
                <input class="form-control" type="text" id="skuBarcode"
                       name="skuBarcode" placeholder="Sku Barcode"
                       value="{{ data.skuBarcode }}">
            </div>
                    <div class="form-group col-md-6">
                <label for="skuStock">Sku Stock</label>
                <input class="form-control" type="text" id="skuStock"
                       name="skuStock" placeholder="Sku Stock"
                       value="{{ data.skuStock }}">
            </div>
            </div>
    {{"productSkusForm" | formToken | raw }}
</form>
